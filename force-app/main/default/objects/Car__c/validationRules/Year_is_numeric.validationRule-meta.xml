<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Year_is_numeric</fullName>
    <active>true</active>
    <errorConditionFormula>IF ( !ISNUMBER( Year__c ) , True, False)</errorConditionFormula>
    <errorDisplayField>Year__c</errorDisplayField>
    <errorMessage>The Year must be a number</errorMessage>
</ValidationRule>
